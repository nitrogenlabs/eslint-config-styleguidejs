{
  "name": "eslint-config-styleguidejs",
  "description": "JS Style Guide ESLint Configuration",
  "version": "4.1.3",
  "type": "module",
  "main": "eslint.config.js",
  "exports": {
    ".": {
      "import": "./index.js"
    }
  },
  "keywords": [
    "babel",
    "eslint",
    "guide",
    "javascript",
    "js",
    "nitrogenlabs",
    "react",
    "style",
    "styleguide",
    "typescript"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenx.co",
    "url": "https://styleguidejs.nitrogenx.co"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/eslint-config-styleguidejs"
  },
  "homepage": "https://github.com/nitrogenlabs/eslint-config-styleguidejs",
  "bugs": {
    "url": "https://github.com/nitrogenlabs/eslint-config-styleguidejs/issues"
  },
  "scripts": {
    "clean": "lex clean",
    "publish:major": "npm version major && npm run publish:tags && npm publish",
    "publish:minor": "npm version minor && npm run publish:tags && npm publish",
    "publish:patch": "npm version patch && npm run publish:tags && npm publish",
    "publish:tags": "git push --tags && git push origin HEAD",
    "test": "node test/run-tests.js",
    "test:rules": "node test/validate-rules-standalone.js",
    "update": "lex update --interactive"
  },
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@stylistic/eslint-plugin": "^5.2.2",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.39.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  }
}
